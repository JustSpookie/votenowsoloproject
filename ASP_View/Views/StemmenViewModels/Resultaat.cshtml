@model ASP_View.Models.ResultaatsModels

@{
    ViewData["Title"] = "Restultaat";
}


<table class="table table-bordered">
    <thead class="thead-light">
        <tr>
            <th>
                KandidaatNaam
            </th>
            <th>
                Stemmen
            </th>
        </tr>
    </thead>
    <tbody>
@for(int i = 0; i < Model.resultaatModels.Count; i++) {
        <tr>
            <td>
                @Model.resultaatModels[i].KandidaatViewModel.KandidaatNaam
            </td>
            <td>
                @Model.resultaatModels[i].Stemmen
            </td>
        </tr>
}
    </tbody>
</table>
